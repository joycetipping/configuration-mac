[alias]
  br    = branch
  cdiff = diff --cached
  ci    = commit -v
  co    = checkout
  dump  = cat-file -p
  hist  = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  st    = status
  type  = cat-file -t
[color]
  diff   = auto
  status = auto
  branch = auto
[push]
	default = simple
